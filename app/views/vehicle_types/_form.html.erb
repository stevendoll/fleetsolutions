<%= simple_form_for(@vehicle_type) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :year %>
    <%= f.input :miles %>
    <%= f.input :lifetime_miles %>
    <%= f.input :mileage %>
    <%= f.input :miles_per_year %>
    <%= f.input :resale_value %>
    <%= f.input :conversion_cost %>
    <%= f.input :propane_factor %>
    <%= f.input :display_order %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
